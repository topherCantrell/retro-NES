                  printString:
                  ; Print a null-terminated string to the screen
                  ;
                  ; GP0 = ROM address of string
                  ; GP1 = Name table address
                  ;
C617: AD 02 20         lda    PPUSTATUS     ; Clear address vram address latch
C61A: A5 03            lda    GP1+1         ; MSB of ...
C61C: 8D 06 20         sta    PPUADDR       ; ... name table address
C61F: A5 02            lda    GP1           ; LSB of ...
C621: 8D 06 20         sta    PPUADDR       ; ... name table address
C624: A0 00            ldy    #0x00         ; First byte of string
                  _printLoop:
C626: B1 00            lda    (GP0),Y       ; Next character
C628: F0 07            beq    _printDone    ; End of string
C62A: 8D 07 20         sta    PPUDATA       ; To screen
C62D: C8               iny                  ; Next character
C62E: 4C 26 C6         jmp    _printLoop    ; Do all
                  _printDone:
C631: 60               rts                  ; Done
                  
                  printHexByte:
                  ; Print a 2 digit hex byte to the screen
                  ;
                  ; A = Value to print
                  ; GP1 = Name table address
                  ;
C632: AA               tax                  ; Save value
C633: AD 02 20         lda    PPUSTATUS     ; Clear address vram address latch
C636: A5 03            lda    GP1+1         ; MSB of ...
