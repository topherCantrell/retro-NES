C166: 4C 19 C1         jmp    main
                  
                  read_byte:
                  ; in-data is 0x4016 bit D0
                  ; clock is 0x4016 bit D3
C169: A2 08            ldx    #8                  ; 8 bits
                  _read_all:
                  ; Wait for clock to go low
                  _read1:
C16B: AD 16 40         lda    APU_JOYPAD1         ; Read controller port
C16E: 29 08            and    #0b0000_1000        ; Wait for CLOCK (inverted) ...
C170: F0 F9            beq    _read1              ; ... to go low
                  ; Wait for clock to go high
                  _read2:
C172: AD 16 40         lda    APU_JOYPAD1         ; Read controller port
C175: 29 08            and    #0b0000_1000        ; Wait for CLOCK (inverted) ...
C177: D0 F9            bne    _read2              ; ... to go high
C179: AD 16 40         lda    APU_JOYPAD1         ; Read controller port
C17C: 49 FF            eor    #255                ; Input is inverted. Flip the bits.
C17E: 6A               ror    a                   ; DATA_IN to carry ...
C17F: 26 00            rol    GP0                 ; ... and to final value
                  ; Repeat
C181: CA               dex                        ; Do all ...
C182: D0 E7            bne    _read_all           ; ... 8 bits
C184: A5 00            lda    GP0                 ; Return the value
C186: 60               rts                        ; Done
                  
                  write_byte:
                  ; out-data is 0x4016 bit D0
                  ; clock is 0x4016 bit D3
C187: 85 00            sta    GP0                 ; The outgoing bits
C189: A2 08            ldx    #8                  ; 8 bits to move
                  _write_all:
                  ; Wait for clock to go low
                  _write1:
C18B: AD 16 40         lda    APU_JOYPAD1         ; Read controller port
C18E: 29 08            and    #0b0000_1000        ; Wait for CLOCK (inverted) ...
C190: F0 F9            beq    _write1             ; ... to go low
                  ; Write the data
C192: 26 00            rol    GP0                 ; Get the next bit ...
C194: 2A               rol    a                   ; ... to A bit 0
C195: 29 01            and    #1                  ; Isolate the bit
C197: 8D 16 40         sta    APU_JOYPAD1         ; Write the bit
                  ; Wait for clock to go high
                  _write3:
C19A: AD 16 40         lda    APU_JOYPAD1         ; Read controller port
C19D: 29 08            and    #0b0000_1000        ; Wait for CLOCK (inverted) ...
C19F: D0 F9            bne    _write3              ; ... to go high
                  ; Repeat
C1A1: CA               dex                        ; Do all ...
C1A2: D0 E7            bne    _write_all          ; ... 8 bits
C1A4: 60               rts                        ; Done
                  
