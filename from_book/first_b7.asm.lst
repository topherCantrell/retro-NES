C63B: A5 02            lda    GP1           ; LSB of ...
C63D: 8D 06 20         sta    PPUADDR       ; ... name table address
C640: 8A               txa                  ; Restore value
C641: 4A               lsr    a             ; High ...
C642: 4A               lsr    a             ; ...
C643: 4A               lsr    a             ; ...
C644: 4A               lsr    a             ; ... nibble
