CAB3: 48               pha                        ; Save ...
CAB4: 8A               txa                        ; ...
CAB5: 48               pha                        ; ...
CAB6: 98               tya                        ; ...
CAB7: 48               pha                        ; ... registers
                  
CAB8: A5 10            lda    nmi_ready           ; 0 means ...
CABA: F0 4B            beq    _ppu_update_end     ; ... skip NMI completely
                  
CABC: C9 01            cmp    #1                  ; 1 means ...
CABE: F0 0A            beq    _do_rendering       ; ... render the screen
                  
                  ; 2 means disable rendering
                  
CAC0: A9 00            lda    #0                  ; Disable all ...
CAC2: 8D 01 20         sta    PPUMASK             ; ... screen rendering
CAC5: 85 10            sta    nmi_ready           ; Skip NMIs until re-enabled
CAC7: 4C 07 CB         jmp    _ppu_update_end     ; Done
                  
                  _do_rendering:
                  
                  ; Copy our OAM data to the PPU
                  
CACA: A2 00            ldx    #0                  ; Address 0x0000 ...
CACC: 8E 03 20         stx    OAMADDR             ; ... in sprite ram
CACF: A9 02            lda    #oam >> 8           ; MSB of ...
CAD1: 8D 14 40         sta    APU_OAMDMA          ; ... our OAM data
                  
                  ; Copy our palette to the PPU
                  
CAD4: A9 88            lda    #0b10001000         ; Set the name table ...
CAD6: 8D 00 20         sta    PPUCTRL             ; ... use (?? this never changes)
CAD9: AD 02 20         lda    PPUSTATUS           ; Clear vblank flag
CADC: A9 3F            lda    #0x3F               ; Set VRAM address ...
CADE: 8D 06 20         sta    PPUADDR             ; ... to ...
CAE1: 8E 06 20         stx    PPUADDR             ; ... 0x3F00
CAE4: A2 00            ldx    #0                  ; Start at index 0
                  _loop:
CAE6: BD 00 03         lda    palette,X           ; From the palette ...
CAE9: 8D 07 20         sta    PPUDATA             ; ... to VRAM
CAEC: E8               inx                        ; Bump our index
CAED: E0 20            cpx    #32                 ; All done?
CAEF: 90 F5            bcc    _loop               ; No, keep going
                  
CAF1: A9 88            lda    #0b10001000         ; Set the name table ...
CAF3: 8D 00 20         sta    PPUCTRL             ; ... use (?? this never changes)
CAF6: A9 00            lda    #0x00
CAF8: 8D 05 20         sta    PPUSCROLL
CAFB: 8D 05 20         sta    PPUSCROLL
                  
CAFE: A9 1E            lda    #0b00011110         ; Enable ...
CB00: 8D 01 20         sta    PPUMASK             ; ... rendering (we might have turned it off with =2)
                  
CB03: A2 00            ldx    #0                  ; Skip NMI from ...
CB05: 86 10            stx    nmi_ready           ; ... now on
                  
                  _ppu_update_end:
CB07: 68               pla                        ; Restore ...
CB08: A8               tay                        ; ...
CB09: 68               pla                        ; ...
CB0A: AA               tax                        ; ...
CB0B: 68               pla                        ; ... registers
CB0C: 40               rti                        ; Return from NMI
                  
